{
  "bin": "./tasks/start-cli.js",
  "name": "beakerbrowser",
  "devDependencies": {
    "asar": "^0.11.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "electron": "1.7.10",
    "electron-builder": "~10.4.1",
    "electron-rebuild": "^1.6.0",
    "eslint": "^4.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-jetpack": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "into-stream": "^2.0.1",
    "q": "^1.4.1",
    "rollup": "^0.41.0"
  },
  "optionalDependencies": {
    "appdmg": "^0.4.5",
    "rcedit": "^0.5.0"
  },
  "build": {
    "appId": "com.pfrazee.beaker-browser",
    "category": "public.app-category.productivity",
    "copyright": "Â© 2017, Blue Link Labs",
    "npmRebuild": false,
    "asar": false,
    "protocols": [
      {
        "name": "URL",
        "schemes": [
          "http",
          "https"
        ]
      },
      {
        "name": "dat",
        "schemes": [
          "dat"
        ]
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 220,
          "type": "file",
          "path": "dist/mac/Beaker Browser.app"
        },
        {
          "x": 50,
          "y": 400,
          "type": "position",
          "path": ".background"
        },
        {
          "x": 150,
          "y": 400,
          "type": "position",
          "path": ".DS_Store"
        },
        {
          "x": 250,
          "y": 400,
          "type": "position",
          "path": ".Trashes"
        },
        {
          "x": 350,
          "y": 400,
          "type": "position",
          "path": ".VolumeIcon.icns"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "cd app && npm install",
    "build": "gulp build",
    "burnthemall": "node ./tasks/burnthemall.js",
    "lint": "eslint --ignore-pattern 'color-thief.js' --ignore-pattern 'app/node_modules' --ignore-pattern '*.build.js' app",
    "rebuild": "electron-rebuild && gulp rebuild",
    "release-mac": "build -m -p never",
    "release-win": "build -w -p never",
    "start": "gulp start",
    "start-logging": "DEBUG=*,-bittorrent-dht $(node -e \"console.log(require('electron'))\") -r trace ./app",
    "watch": "gulp start-watch",
    "break": "gulp break"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/beaker",
    "logo": "https://opencollective.com/opencollective/logo.txt"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "atob": "^2.0.3",
    "base32": "0.0.6",
    "file-type": "^7.4.0",
    "muxrpc": "^6.4.0",
    "pauls-electron-rpc": "^4.1.0",
    "pull-concat": "^1.1.1",
    "pull-identify-filetype": "^1.1.0",
    "pull-pause": "0.0.1",
    "ssb-afterparty": "0.0.1",
    "ssb-party": "^0.5.1",
    "stream-to-buffer": "^0.1.0",
    "yargs": "^4.2.0"
  }
